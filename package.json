{
  "name": "@decisively-io/interview-sdk",
  "version": "0.4.0-alpha.7",
  "description": "An SDK for the Decisively interview system.",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run check && npm run build",
    "clean": "rimraf dist && rimraf docs",
    "doc": "typedoc --plugin typedoc-plugin-markdown --plugin typedoc-github-wiki-theme",
    "check:fix": "tsc --noEmit --project tsconfig.json && npx @biomejs/biome check --apply-unsafe .",
    "check": "tsc --noEmit --project tsconfig.json && npx @biomejs/biome check .",
    "build": "yarn clean && tsc && npm run doc",
    "dev": "tsc -w",
    "deploy": "yarn build && yarn publish"
  },
  "files": [
    "dist",
    "docs"
  ],
  "keywords": [
    "decisively",
    "interview",
    "api",
    "sdk",
    "typescript"
  ],
  "author": "Decisively",
  "license": "UNLICENSED",
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@types/lodash.isempty": "^4.4.8",
    "@types/lodash.isequal": "^4.5.7",
    "@types/uuid": "^9.0.6",
    "rimraf": "^5.0.5",
    "typedoc": "^0.25.2",
    "typedoc-github-wiki-theme": "^1.1.0",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@decisively-io/types-interview": "^0.1.27",
    "axios": "^0.26.1",
    "immer": "^9.0.12",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "rxjs": "^7.5.5",
    "uuid": "^8.3.2"
  }
}
